{
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register",
    "source-map-support/register"
  ],
  "exclude": [
    "**/*.d.ts",
    "**/*.spec.ts",
    "dist/*",
    "coverage/*",
    "playground/*",
    "reports/*",
    "test*",
    "src/**/index.ts",
    "index.ts",
    ".eslintrc.js"
  ],
  "reporter": [
    "html",
    "lcov"
  ],
  "all": true
}
